{
  "name": "emeraude",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "build:preprod": "tsc && vite build --mode preprod",
    "build:production": "tsc && vite build --mode production",
    "build:staging": "tsc && vite build --mode staging",
    "build:scorm:preprod": "sh cmd/build-scorm-preprod.sh",
    "build:scorm:production": "sh cmd/build-scorm-production.sh",
    "build:scorm:staging": "sh cmd/build-scorm-staging.sh",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "dev": "vite --host",
    "dist:package": "node ./bin/create-package.cjs",
    "episodes:converse": "npm run build:scripts && node scripts/dist/episodes-converse.js",
    "i18n:add-json-file": "npm run build:scripts && node scripts/dist/i18n-add-json-file.js",
    "i18n:build-schema": "npm run build:scripts && node scripts/dist/i18n-build-schema-from-en.js",
    "i18n:export": "npm run build:scripts && node scripts/dist/i18n-export.js",
    "i18n:gen": "npm run build:scripts && node scripts/dist/i18n-schema-gen.js",
    "i18n:import": "npm run build:scripts && node scripts/dist/i18n-import.js",
    "i18n:merge-locales": "npm run build:scripts && node scripts/dist/i18n-merge-locales.js",
    "i18n:validate-file-names": "npm run build:scripts && node scripts/dist/file-name-validator.js",
    "i18n:validate-schema": "npm run build:scripts && node scripts/dist/i18n-validate-schema-to-languages.js",
    "i18n:validate-translate-pipe": "npm run build:scripts && node scripts/dist/i18n-validate-translate-pipe.js",
    "i18n:validate-translate-usage": "npm run build:scripts && node scripts/dist/i18n-validate-translation-usage.js",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "production": "vite --mode production",
    "scorm-manifest": "npm run build:scripts && node scripts/dist/scorm-manifest.js",
    "scorm-manifest-discard": "git checkout HEAD -- ./bin/imsmanifest.xml",
    "scorm-manifest:production": "npm run build:scripts && node scripts/dist/scorm-manifest.js production",
    "scorm-manifest:preprod": "npm run build:scripts && node scripts/dist/scorm-manifest.js preprod",
    "scorm-manifest:staging": "npm run build:scripts && node scripts/dist/scorm-manifest.js staging",
    "textid": "node ./vite.generateTextId.js start",
    "schema": "node ./vite.generateSchema.js start"
  },
