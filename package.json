    "mini-css-extract-plugin": "^2.9.1",
    "pancake": "file:libs/pancake",
    "peppermill": "file:libs/peppermill",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "raw-loader": "^4.0.2",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "spices": "file:libs/spices",
    "storyblok-rich-text-react-renderer": "^2.9.2",
    "tailwindcss": "^3.4.10",
    "ts-loader": "^9.5.1",
